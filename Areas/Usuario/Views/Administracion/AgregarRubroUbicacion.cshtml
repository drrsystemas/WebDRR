@model LibreriaBase.Areas.Usuario.DatoConfiguracion

@{
    ViewData["Title"] = "AgregarRubroUbicacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Agregar Rubro Ubicacion/es</h4>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AgregarRubroUbicacion" asp-controller="Administracion" asp-area="Usuario">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="Codigo" class="control-label"></label>
                <input asp-for="Codigo" class="form-control" />
                <span asp-validation-for="Codigo" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label class="control-label">Nombre</label>
                <input  asp-for="Type" class="form-control" />
                <span asp-validation-for="Type" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="Descripcion" class="control-label">Ubicacion</label>
                <input asp-for="Descripcion" class="form-control" />
                <span asp-validation-for="Descripcion" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label class="control-label">Orden</label>
                <input  asp-for="Extra" class="form-control" />
                <span asp-validation-for="Extra" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label class="control-label">Incluye SubRubros</label>
                <input  asp-for="ExtraDos" class="form-control" />
                <span asp-validation-for="ExtraDos" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Aceptar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<br />
<br />
<br />


@section Scripts
{

}
